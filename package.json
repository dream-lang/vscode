{
  "name": "dream-lang-vscode",
  "displayName": "dream",
  "description": "VSCode Support For Dream Programming Language",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:dream-lang/vscode.git"
  },
  "publisher": "dream-lang",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "dream",
        "aliases": [
          "Dream",
          "dream",
          "Dream Lang",
          "dream-lang"
        ],
        "extensions": [
          ".dm",
          ".dream"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dream",
        "scopeName": "index.dm",
        "path": "./syntaxes/dream.tmLanguage.json"
      }
    ]
  }
}
